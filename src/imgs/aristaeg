strict digraph {
	graph [rankdir=LR]
	ini [shape=point]
	ini -> a
	a [label=a]
	b [label=b]
	c [label=c]
	d [label=d]
	e [label=e]
	f [color=blue shape=doublecircle size="1,1"]
	f [label=f]
	g [label=g]
	a -> a [label=0 color=red]
	a -> c [label=1 color=red]
	b -> b [label=0 color=red]
	b -> e [label=1 color=red]
	c -> b [label=1 color=red]
	c -> d [label=0 color=red]
	d -> f [label=1 color=red]
	d -> e [label=0 color=red]
	e -> f [label=1 color=red]
	e -> g [label=0 color=red]
}
